{
	"name": "@snickbit/feathers-files",
	"description": "Files adapter for FeathersJS",
	"version": "1.0.25",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"build": "tsup",
		"meta": "npm run prod -- --metafile",
		"prod": "npm run build -- --minify",
		"dev": "npm run build -- --sourcemap",
		"watch": "npm run dev -- --watch",
		"index": "indexer src",
		"depcheck": "depcheck"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.67.0",
		"@feathersjs/adapter-commons": "5.0.0-pre.18",
		"@feathersjs/errors": "5.0.0-pre.18",
		"@feathersjs/feathers": "5.0.0-pre.18",
		"@snickbit/feathers-helpers": "^0.0.40",
		"@snickbit/node-utilities": "^4.2.32",
		"@snickbit/out": "^2.0.20",
		"@snickbit/utilities": "^3.0.16",
		"@types/express": "^4.17.13",
		"express": "^4.18.0",
		"fast-glob": "^3.2.11",
		"fs-extra": "^10.1.0",
		"mime": "^3.0.0",
		"multer": "^1.4.4",
		"sift": "^16.0.0"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.18",
		"@remedyred/indexer": "^2.0.77",
		"@types/jest": "^27.4.1",
		"@types/multer": "^1.4.7",
		"jest": "^27.5.1",
		"shx": "^0.3.4",
		"ts-jest": "^27.1.4",
		"tsup": "^5.12.6",
		"typescript": "^4.6.3"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	},
	"nx": {
		"implicitDependencies": [
			"@snickbit/feathers-helpers",
			"@snickbit/node-utilities",
			"@snickbit/out",
			"@snickbit/utilities"
		]
	}
}
