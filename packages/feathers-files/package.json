{
	"name": "@snickbit/feathers-files",
	"version": "2.0.12",
	"description": "Files adapter for FeathersJS",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"dev": "pnpm run build --sourcemap",
		"index": "indexer",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run build --minify",
		"watch": "pnpm run dev --watch"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.100.0",
		"@feathersjs/adapter-commons": "5.0.0-pre.22",
		"@feathersjs/errors": "5.0.0-pre.22",
		"@feathersjs/feathers": "5.0.0-pre.22",
		"@snickbit/feathers-helpers": "^1.0.10",
		"@snickbit/node-utilities": "^4.3.11",
		"@snickbit/out": "^2.0.31",
		"@snickbit/utilities": "^3.0.25",
		"@types/express": "^4.17.13",
		"express": "^4.18.1",
		"fast-glob": "^3.2.11",
		"fs-extra": "^10.1.0",
		"mime": "^3.0.0",
		"multer": "^1.4.4",
		"sift": "^16.0.0"
	},
	"devDependencies": {
		"@remedyred/indexer": "^2.4.2",
		"@types/jest": "^27.5.2",
		"@types/multer": "^1.4.7",
		"jest": "^28.1.0",
		"shx": "^0.3.4",
		"ts-jest": "^28.0.4",
		"tsup": "^6.0.1",
		"typescript": "^4.7.2"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.22"
	},
	"engines": {
		"node": ">= 12"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
