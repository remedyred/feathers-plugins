{
	"name": "@snickbit/feathers-files",
	"version": "2.0.29",
	"description": "Files adapter for FeathersJS",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify --dts",
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"dev": "tsup",
		"docs": "typedoc src/index.ts --tsconfig ./tsconfig.typedoc.json",
		"release": "semantic-release",
		"types": "tsc --project ./tsconfig.types.json",
		"watch": "pnpm run dev --watch"
	},
	"release": {
		"extends": [
			"@snickbit/semantic-release"
		]
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.241.0",
		"@feathersjs/adapter-commons": "5.0.0-pre.26",
		"@feathersjs/errors": "5.0.0-pre.26",
		"@feathersjs/feathers": "5.0.0-pre.26",
		"@snickbit/feathers-helpers": "1.0.20",
		"@snickbit/node-utilities": "4.4.14",
		"@snickbit/out": "2.0.48",
		"@snickbit/utilities": "3.4.13",
		"express": "4.18.1",
		"fast-glob": "3.2.12",
		"fs-extra": "10.1.0",
		"mime": "3.0.0",
		"multer": "1.4.5-lts.1",
		"sift": "16.0.0"
	},
	"devDependencies": {
		"@snickbit/indexer": "^2.5.27",
		"@types/express": "4.17.14",
		"@types/multer": "~1.4.7",
		"tsup": "6.2.3",
		"typedoc": "0.23.15",
		"typedoc-plugin-markdown": "3.13.6",
		"typescript": "4.9.5"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.26"
	},
	"engines": {
		"node": ">= 12"
	}
}
