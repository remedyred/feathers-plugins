{
	"name": "@snickbit/feathers-rest",
	"description": "Feathers service for interactive with REST APIs",
	"version": "0.0.38",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"build": "tsup",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run build --minify",
		"dev": "pnpm run build --sourcemap",
		"watch": "pnpm run dev --watch"
	},
	"dependencies": {
		"@feathersjs/adapter-commons": "5.0.0-pre.18",
		"@feathersjs/feathers": "5.0.0-pre.18",
		"@snickbit/feathers-helpers": "^0.0.44",
		"@snickbit/out": "^2.0.25",
		"axios": "^0.27.2"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.18",
		"jest": "^27.5.1",
		"tsup": "^5.12.7"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	},
	"nx": {
		"implicitDependencies": [
			"@snickbit/feathers-helpers",
			"@snickbit/out"
		]
	}
}
