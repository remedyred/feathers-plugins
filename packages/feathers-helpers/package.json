{
	"name": "@snickbit/feathers-helpers",
	"version": "1.0.16",
	"description": "Helper functions for FeathersJS",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"dev": "pnpm run build --sourcemap",
		"index": "indexer",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run build --minify",
		"watch": "pnpm run dev --watch"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@feathersjs/adapter-commons": "5.0.0-pre.26",
		"@feathersjs/commons": "5.0.0-pre.26",
		"@feathersjs/errors": "5.0.0-pre.26",
		"@snickbit/out": "^2.0.36",
		"@snickbit/utilities": "^3.1.0",
		"sift": "^16.0.0"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.26",
		"@snickbit/indexer": "^2.5.5",
		"@types/jest": "^28.1.3",
		"jest": "^28.1.1",
		"shx": "^0.3.4",
		"ts-jest": "^28.0.5",
		"tsup": "^6.1.2",
		"typescript": "^4.7.4"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.26"
	},
	"engines": {
		"node": ">= 12"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
