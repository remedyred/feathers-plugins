{
	"name": "@snickbit/feathers-helpers",
	"description": "Helper functions for FeathersJS",
	"version": "0.0.36",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"build": "tsup",
		"build:meta": "tsup --minify --metafile",
		"prod": "tsup --minify",
		"watch": "tsup --watch",
		"index": "indexer src",
		"depcheck": "depcheck"
	},
	"dependencies": {
		"@feathersjs/adapter-commons": "^5.0.0-pre.18",
		"@feathersjs/commons": "^5.0.0-pre.18",
		"@feathersjs/errors": "^5.0.0-pre.18",
		"@snickbit/out": "^2.0.5",
		"@snickbit/utilities": "^3.0.5",
		"sift": "^16.0.0"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "^5.0.0-pre.18"
	},
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"jest": "^27.5.1",
		"shx": "^0.3.4",
		"ts-jest": "^27.1.4",
		"@feathersjs/feathers": "^5.0.0-pre.18",
		"tsup": "^5.12.6",
		"typescript": "^4.6.3"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
