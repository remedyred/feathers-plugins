{
	"name": "@snickbit/feathers-logger",
	"desciption": "Client for @snickbit/feathers-logs",
	"version": "0.1.18",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"build": "tsup",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run build --minify",
		"dev": "pnpm run build --sourcemap",
		"watch": "pnpm run dev --watch"
	},
	"dependencies": {
		"@snickbit/out": "^2.0.25",
		"@snickbit/utilities": "^3.0.17",
		"axios": "^0.27.2",
		"browser-or-node": "^2.0.0"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.18",
		"@remedyred/indexer": "^2.1.1",
		"@types/node": "^17.0.31",
		"jest": "^27.5.1",
		"tsup": "^5.12.7",
		"typescript": "^4.6.4"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	},
	"nx": {
		"implicitDependencies": [
			"@snickbit/out",
			"@snickbit/utilities"
		]
	}
}
