{
	"name": "@snickbit/feathers-model",
	"description": "Data models for FeathersJS",
	"version": "0.0.37",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"build": "tsup",
		"build:meta": "tsup --metafile",
		"prod": "tsup --minify",
		"watch": "tsup --watch"
	},
	"dependencies": {
		"@feathersjs/adapter-commons": "^4.5.12",
		"@feathersjs/errors": "^4.5.12",
		"@snickbit/feathers-helpers": "^0.0.29",
		"@snickbit/model": "^1.0.22",
		"@snickbit/out": "^2.0.7",
		"@snickbit/utilities": "^3.0.7",
		"feathers-memory": "^4.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.17.9",
		"@babel/preset-env": "^7.16.11",
		"@remedyred/indexer": "^2.0.72",
		"babel-jest": "^27.5.1",
		"babel-plugin-add-module-exports": "^1.0.4",
		"jest": "^27.5.1",
		"tsup": "^5.12.6"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "^4.5.12"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
