{
	"name": "@snickbit/feathers-up-mongodb",
	"version": "0.0.28",
	"description": "MongoDB plugin for @snickbit/feathers-up",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify --dts",
		"clean": "shx rm -rf ./dist ./.types",
		"depcheck": "depcheck",
		"dev": "tsup",
		"docs": "typedoc src/index.ts --tsconfig ./tsconfig.typedoc.json",
		"release": "semantic-release",
		"test": "jest",
		"watch": "pnpm run dev --watch"
	},
	"release": {
		"extends": [
			"semantic-release-monorepo",
			"@snickbit/semantic-release"
		]
	},
	"dependencies": {
		"@snickbit/feathers-up": "^1.0.33",
		"mongodb": "^4.7.0"
	},
	"devDependencies": {
		"tsup": "6.2.2"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.26"
	},
	"engines": {
		"node": ">= 12"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		],
		"external": [
			"mongodb"
		]
	}
}
