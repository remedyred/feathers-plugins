{
	"name": "@snickbit/feathers-hooks",
	"version": "1.0.9",
	"description": "Dynamic hook loader for Feathers",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify --dts",
		"clean": "shx rm -rf ./dist ./.types",
		"depcheck": "depcheck",
		"dev": "tsup",
		"docs": "typedoc src/index.ts --tsconfig ./tsconfig.typedoc.json",
		"release": "semantic-release",
		"watch": "pnpm run dev --watch"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.26",
		"@snickbit/indexer": "2.5.7",
		"tsup": "6.2.2"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.26"
	},
	"engines": {
		"node": ">= 12"
	}
}
