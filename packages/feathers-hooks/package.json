{
	"name": "@snickbit/feathers-hooks",
	"version": "1.0.11",
	"description": "Dynamic hook loader for Feathers",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify --dts",
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"dev": "tsup",
		"docs": "typedoc src/index.ts --tsconfig ./tsconfig.typedoc.json",
		"release": "semantic-release",
		"types": "tsc --project ./tsconfig.types.json",
		"watch": "pnpm run dev --watch"
	},
	"release": {
		"extends": [
			"@snickbit/semantic-release"
		]
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.26",
		"@snickbit/indexer": "^2.5.27",
		"tsup": "6.2.3",
		"typedoc": "0.23.26",
		"typedoc-plugin-markdown": "3.13.6",
		"typescript": "4.8.4"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.26"
	},
	"engines": {
		"node": ">= 12"
	}
}
