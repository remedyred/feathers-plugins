{
	"name": "@snickbit/feathers-hooks",
	"description": "Dynamic hook loader for Feathers",
	"version": "0.0.18",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"build": "tsup",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run build --minify",
		"dev": "pnpm run build --sourcemap",
		"watch": "pnpm run dev --watch",
		"index": "indexer"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.20",
		"@remedyred/indexer": "^2.2.6",
		"jest": "^27.5.1",
		"tsup": "^5.12.7"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
