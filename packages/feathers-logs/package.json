{
	"name": "@snickbit/feathers-logs",
	"version": "1.0.25",
	"description": "A Feathers.js service that logs events to MongoDB",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify --dts",
		"clean": "shx rm -rf ./dist ./.types",
		"depcheck": "depcheck",
		"dev": "tsup",
		"docs": "typedoc src/index.ts --tsconfig ./tsconfig.typedoc.json",
		"release": "semantic-release",
		"watch": "pnpm run dev --watch"
	},
	"release": {
		"extends": [
			"@snickbit/semantic-release"
		]
	},
	"dependencies": {
		"@snickbit/feathers-logger": "~1.0.16",
		"@snickbit/feathers-mongo": "~2.0.8"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.26",
		"tsup": "6.2.3",
		"typedoc": "0.23.12",
		"typedoc-plugin-markdown": "3.13.5",
		"typescript": "4.8.2"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.26"
	},
	"engines": {
		"node": ">= 12"
	}
}
