{
	"name": "@snickbit/feathers-logs",
	"version": "1.0.12",
	"description": "A Feathers.js service that logs events to MongoDB",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"dev": "pnpm run build --sourcemap",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run build --minify",
		"watch": "pnpm run dev --watch"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"dependencies": {
		"@snickbit/feathers-logger": "workspace:^1.0.6",
		"@snickbit/feathers-mongo": "workspace:^1.0.11"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.22",
		"jest": "^28.1.0",
		"tsup": "^6.0.1"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.22"
	},
	"engines": {
		"node": ">= 12"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
