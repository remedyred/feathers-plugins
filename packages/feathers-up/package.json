{
	"name": "@snickbit/feathers-up",
	"description": "Boot manager for FeathersJS with sane defaults",
	"version": "0.1.7",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"build": "tsup",
		"build:meta": "tsup --minify --metafile",
		"prod": "tsup --minify",
		"watch": "tsup --watch"
	},
	"dependencies": {
		"@feathersjs/configuration": "^5.0.0-pre.18",
		"@feathersjs/express": "^5.0.0-pre.18",
		"@feathersjs/feathers": "^5.0.0-pre.18",
		"@feathersjs/socketio": "^5.0.0-pre.18",
		"@sentry/node": "^6.19.6",
		"@snickbit/feathers-hooks": "^0.0.10",
		"@snickbit/feathers-logs": "^0.1.9",
		"@snickbit/model": "^1.0.22",
		"@snickbit/node-utilities": "^4.2.13",
		"@snickbit/out": "^2.0.7",
		"@snickbit/utilities": "^3.0.7",
		"body-parser": "^1.20.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"express": "^4.18.0",
		"helmet": "^5.0.2",
		"ioredis": "^5.0.4",
		"knex": "^1.0.7",
		"node-ray": "^1.18.0"
	},
	"devDependencies": {
		"@feathersjs/feathers": "^5.0.0-pre.18",
		"@remedyred/indexer": "^2.0.72",
		"jest": "^27.5.1",
		"tsup": "^5.12.6"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.js"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
