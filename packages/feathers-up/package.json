{
	"name": "@snickbit/feathers-up",
	"description": "Boot manager for FeathersJS with sane defaults",
	"version": "0.1.12",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"build": "tsup",
		"dev": "npm run build -- --sourcemap",
		"meta": "npm run prod -- --metafile",
		"prod": "npm run build -- --minify",
		"watch": "npm run dev -- --watch"
	},
	"dependencies": {
		"@feathersjs/configuration": "5.0.0-pre.18",
		"@feathersjs/express": "5.0.0-pre.18",
		"@feathersjs/feathers": "5.0.0-pre.18",
		"@feathersjs/socketio": "5.0.0-pre.18",
		"@sentry/node": "^6.19.6",
		"@snickbit/feathers-hooks": "^0.0.16",
		"@snickbit/feathers-logger": "^0.1.15",
		"@snickbit/feathers-logs": "^0.1.14",
		"@snickbit/model": "^1.0.35",
		"@snickbit/node-utilities": "^4.3.1",
		"@snickbit/out": "^2.0.20",
		"@snickbit/utilities": "^3.0.17",
		"body-parser": "^1.20.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"express": "^4.18.0",
		"helmet": "^5.0.2",
		"ioredis": "^5.0.4",
		"knex": "^2.0.0",
		"node-ray": "^1.18.0",
		"socket.io": "^4.5.0"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.18",
		"@remedyred/indexer": "^2.0.79",
		"jest": "^27.5.1",
		"tsup": "^5.12.7"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	},
	"nx": {
		"implicitDependencies": [
			"@snickbit/feathers-hooks",
			"@snickbit/feathers-logger",
			"@snickbit/feathers-logs",
			"@snickbit/model",
			"@snickbit/node-utilities",
			"@snickbit/out",
			"@snickbit/utilities"
		]
	}
}
