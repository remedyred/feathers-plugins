{
	"name": "@snickbit/feathers-up",
	"version": "1.0.33",
	"description": "Boot manager for FeathersJS with sane defaults",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify --dts",
		"clean": "shx rm -rf ./dist ./.types",
		"depcheck": "depcheck",
		"dev": "tsup",
		"docs": "typedoc src/index.ts --tsconfig ./tsconfig.typedoc.json",
		"release": "semantic-release",
		"watch": "pnpm run dev --watch"
	},
	"release": {
		"extends": [
			"semantic-release-monorepo",
			"@snickbit/semantic-release"
		]
	},
	"dependencies": {
		"@feathersjs/configuration": "5.0.0-pre.26",
		"@feathersjs/express": "5.0.0-pre.26",
		"@feathersjs/feathers": "5.0.0-pre.26",
		"@feathersjs/socketio": "5.0.0-pre.26",
		"@sentry/node": "^7.2.0",
		"@snickbit/feathers-hooks": "^1.0.9",
		"@snickbit/feathers-logger": "^1.0.16",
		"@snickbit/feathers-logs": "^1.0.24",
		"@snickbit/model": "^1.0.52",
		"@snickbit/node-utilities": "^4.4.2",
		"@snickbit/out": "^2.0.37",
		"@snickbit/utilities": "^3.2.0",
		"body-parser": "^1.20.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"express": "^4.18.1",
		"helmet": "^5.1.0",
		"socket.io": "^4.5.1",
		"winston": "^3.8.0",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"@snickbit/indexer": "2.5.7",
		"tsup": "6.2.3",
		"typedoc": "0.23.12",
		"typedoc-plugin-markdown": "3.13.5",
		"typescript": "4.8.2"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.26"
	},
	"optionalDependencies": {
		"ioredis": "^5.0.6",
		"knex": "^2.1.0",
		"mongodb": "^4.7.0",
		"mysql": "^2.18.1"
	},
	"engines": {
		"node": ">= 12"
	}
}
