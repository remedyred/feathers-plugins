{
	"name": "@snickbit/feathers-up",
	"version": "1.0.16",
	"description": "Boot manager for FeathersJS with sane defaults",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"dev": "pnpm run build --sourcemap",
		"index": "indexer",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run build --minify",
		"watch": "pnpm run dev --watch"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"dependencies": {
		"@feathersjs/configuration": "5.0.0-pre.22",
		"@feathersjs/express": "5.0.0-pre.22",
		"@feathersjs/feathers": "5.0.0-pre.22",
		"@feathersjs/socketio": "5.0.0-pre.22",
		"@sentry/node": "^6.19.7",
		"@snickbit/feathers-hooks": "workspace:^1.0.4",
		"@snickbit/feathers-logger": "^1.0.7",
		"@snickbit/feathers-logs": "^1.0.13",
		"@snickbit/model": "^1.0.46",
		"@snickbit/node-utilities": "^4.3.10",
		"@snickbit/out": "^2.0.30",
		"@snickbit/utilities": "^3.0.24",
		"body-parser": "^1.20.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"express": "^4.18.1",
		"helmet": "^5.1.0",
		"socket.io": "^4.5.1",
		"winston": "^3.7.2",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"@remedyred/indexer": "^2.4.1",
		"jest": "^28.1.0",
		"tsup": "^6.0.1"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.22"
	},
	"optionalDependencies": {
		"@snickbit/feathers-up-mongodb": "^0.0.0",
		"@snickbit/feathers-up-mysql": "^0.0.0",
		"@snickbit/feathers-up-redis": "^0.0.0",
		"ioredis": "^5.0.6",
		"knex": "^2.1.0",
		"mongodb": "^4.6.0",
		"mysql": "^2.18.1"
	},
	"engines": {
		"node": ">= 12"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		],
		"external": [
			"mysql"
		]
	}
}
