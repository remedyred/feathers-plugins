{

	"indexes": [
		{
			"source": "src/commands/*.ts",
			"output": "src/commands/index.ts",
			"type": "group"
		},
		{
			"source": [
				"src/tasks/*.ts",
				"src/service.ts",
				"src/commands/index.ts"
			],
			"output": "src/index.ts",
			"overrides": {
				"src/commands/index.ts": "group"
			},
			"include": [
				"src/commands/index.ts"
			]
		}
	]
}
