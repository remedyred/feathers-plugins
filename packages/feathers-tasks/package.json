{
	"name": "@snickbit/feathers-tasks",
	"description": "Task management for FeathersJS",
	"version": "1.0.34",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"index": "indexer src",
		"depcheck": "depcheck",
		"build": "tsup",
		"dev": "npm run build -- --sourcemap",
		"meta": "npm run prod -- --metafile",
		"prod": "npm run build -- --minify",
		"watch": "npm run dev -- --watch"
	},
	"dependencies": {
		"@feathersjs/adapter-commons": "5.0.0-pre.18",
		"@feathersjs/errors": "5.0.0-pre.18",
		"@feathersjs/feathers": "5.0.0-pre.18",
		"@snickbit/dates": "^1.0.33",
		"@snickbit/feathers-helpers": "^0.0.40",
		"@snickbit/feathers-model": "^0.0.43",
		"@snickbit/node-cli": "^1.0.42",
		"@snickbit/node-utilities": "^4.2.33",
		"@snickbit/out": "^2.0.20",
		"@snickbit/utilities": "^3.0.16",
		"bullmq": "^1.80.1",
		"ioredis": "^5.0.4"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.18",
		"@remedyred/indexer": "^2.0.77",
		"esbuild": "^0.14.36",
		"jest": "^27.5.1",
		"tsup": "^5.12.6"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	},
	"nx": {
		"implicitDependencies": [
			"@snickbit/dates",
			"@snickbit/feathers-helpers",
			"@snickbit/feathers-model",
			"@snickbit/node-cli",
			"@snickbit/node-utilities",
			"@snickbit/out",
			"@snickbit/utilities"
		]
	}
}
