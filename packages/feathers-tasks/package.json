{
	"name": "@snickbit/feathers-tasks",
	"version": "2.0.34",
	"description": "Task management for FeathersJS",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
