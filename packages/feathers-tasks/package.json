{
	"name": "@snickbit/feathers-tasks",
	"description": "Task management for FeathersJS",
	"version": "1.0.28",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"build": "tsup",
		"build:meta": "tsup --minify --metafile",
		"prod": "tsup --minify",
		"watch": "tsup --watch"
	},
	"dependencies": {
		"@feathersjs/adapter-commons": "^5.0.0-pre.18",
		"@feathersjs/errors": "^5.0.0-pre.18",
		"@feathersjs/feathers": "^5.0.0-pre.18",
		"@snickbit/dates": "^1.0.28",
		"@snickbit/feathers-helpers": "^0.0.29",
		"@snickbit/feathers-model": "^0.0.37",
		"@snickbit/node-cli": "^1.0.26",
		"@snickbit/node-utilities": "^4.2.13",
		"@snickbit/out": "^2.0.7",
		"@snickbit/utilities": "^3.0.7",
		"bullmq": "^1.80.1",
		"ioredis": "^5.0.4"
	},
	"devDependencies": {
		"@feathersjs/feathers": "^5.0.0-pre.18",
		"@remedyred/indexer": "^2.0.72",
		"esbuild": "^0.14.36",
		"jest": "^27.5.1",
		"tsup": "^5.12.6"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.js"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
