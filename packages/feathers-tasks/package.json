{
	"name": "@snickbit/feathers-tasks",
	"version": "2.0.16",
	"description": "Task management for FeathersJS",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"dev": "pnpm run build --sourcemap",
		"index": "indexer",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run build --minify",
		"watch": "pnpm run dev --watch"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"dependencies": {
		"@feathersjs/adapter-commons": "5.0.0-pre.22",
		"@feathersjs/errors": "5.0.0-pre.22",
		"@feathersjs/feathers": "5.0.0-pre.22",
		"@snickbit/dates": "^1.0.45",
		"@snickbit/feathers-helpers": "^1.0.11",
		"@snickbit/feathers-model": "^1.0.12",
		"@snickbit/node-cli": "^1.0.55",
		"@snickbit/node-utilities": "^4.3.12",
		"@snickbit/out": "^2.0.32",
		"@snickbit/utilities": "^3.0.26",
		"bullmq": "^1.85.3",
		"ioredis": "^5.0.6"
	},
	"devDependencies": {
		"@remedyred/indexer": "^2.4.2",
		"esbuild": "^0.14.42",
		"jest": "^28.1.0",
		"tsup": "^5.12.9"
	},
	"peerDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.22"
	},
	"engines": {
		"node": ">= 12"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
