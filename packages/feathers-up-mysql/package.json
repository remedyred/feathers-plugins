{
	"name": "@snickbit/feathers-up-mysql",
	"version": "0.0.29",
	"description": "MySQL/Knex plugin for @snickbit/feathers-up",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify --dts",
		"clean": "shx rm -rf ./dist ./.types",
		"depcheck": "depcheck",
		"dev": "tsup",
		"docs": "typedoc src/index.ts --tsconfig ./tsconfig.typedoc.json",
		"release": "pnpm dlx semantic-release",
		"watch": "pnpm run dev --watch"
	},
	"release": {
		"extends": [
			"semantic-release-monorepo",
			"@snickbit/semantic-release"
		]
	},
	"dependencies": {
		"@snickbit/feathers-up": "~1.0.33",
		"knex": "2.3.0",
		"mysql": "2.18.1"
	},
	"devDependencies": {
		"tsup": "6.2.3",
		"typedoc": "0.23.12",
		"typedoc-plugin-markdown": "3.13.5",
		"typescript": "4.8.2"
	},
	"engines": {
		"node": ">= 12"
	}
}
