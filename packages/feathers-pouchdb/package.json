{
	"name": "@snickbit/feathers-pouchdb",
	"version": "0.0.15",
	"description": "PouchDB adapter for FeathersJS",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify --dts",
		"clean": "shx rm -rf ./dist ./.types",
		"depcheck": "depcheck",
		"dev": "tsup",
		"docs": "typedoc src/index.ts --tsconfig ./tsconfig.typedoc.json",
		"release": "semantic-release",
		"watch": "pnpm run dev --watch"
	},
	"release": {
		"extends": [
			"@snickbit/semantic-release"
		]
	},
	"dependencies": {
		"@feathersjs/adapter-commons": "5.0.0-pre.26",
		"@feathersjs/errors": "5.0.0-pre.26",
		"@feathersjs/feathers": "5.0.0-pre.26",
		"@snickbit/feathers-helpers": "~1.0.18",
		"@snickbit/out": "~2.0.37",
		"@snickbit/utilities": "~3.4.0",
		"garbados-crypt": "3.0.0-beta",
		"pouchdb": "7.3.0",
		"pouchdb-adapter-memory": "7.3.0",
		"pouchdb-find": "7.3.0",
		"pouchdb-md5": "7.3.0",
		"transform-pouch": "2.0.0",
		"tweetnacl": "1.0.3",
		"tweetnacl-util": "0.15.1"
	},
	"devDependencies": {
		"@types/pouchdb": "6.4.0",
		"shx": "0.3.4",
		"tsup": "6.2.3",
		"typedoc": "0.23.15",
		"typedoc-plugin-markdown": "3.13.6",
		"typescript": "4.8.4"
	},
	"engines": {
		"node": ">= 12"
	}
}
