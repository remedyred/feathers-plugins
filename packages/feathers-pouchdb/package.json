{
	"name": "@snickbit/feathers-pouchdb",
	"version": "0.0.6",
	"description": "PouchDB adapter for FeathersJS",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"clean": "shx rm -rf ./dist ",
		"depcheck": "depcheck",
		"dev": "pnpm run build --sourcemap",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run clean && pnpm run build --minify",
		"test": "jest",
		"watch": "pnpm run dev --watch"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@feathersjs/adapter-commons": "5.0.0-pre.22",
		"@feathersjs/errors": "5.0.0-pre.22",
		"@feathersjs/feathers": "5.0.0-pre.22",
		"@snickbit/feathers-helpers": "^1.0.10",
		"@snickbit/out": "^2.0.31",
		"@snickbit/utilities": "^3.0.25",
		"garbados-crypt": "3.0.0-beta",
		"pouchdb": "^7.3.0",
		"pouchdb-adapter-memory": "^7.3.0",
		"pouchdb-find": "^7.3.0",
		"pouchdb-md5": "^7.3.0",
		"transform-pouch": "^2.0.0",
		"tweetnacl": "^1.0.3",
		"tweetnacl-util": "^0.15.1"
	},
	"devDependencies": {
		"@types/jest": "^27.5.2",
		"@types/pouchdb": "^6.4.0",
		"jest": "^27.5.1",
		"shx": "^0.3.4",
		"ts-jest": "^27.1.5",
		"tsup": "^5.12.9",
		"typescript": "^4.7.2"
	},
	"engines": {
		"node": ">= 12"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
