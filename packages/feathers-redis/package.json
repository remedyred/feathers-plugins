{
	"name": "@snickbit/feathers-redis",
	"description": "Feathers Redis Service",
	"version": "0.0.37",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"depcheck": "depcheck",
		"build": "tsup",
		"meta": "npm run prod -- --metafile",
		"prod": "npm run build -- --minify",
		"dev": "npm run build -- --sourcemap",
		"watch": "npm run dev -- --watch"
	},
	"dependencies": {
		"@feathersjs/adapter-commons": "5.0.0-pre.18",
		"@feathersjs/errors": "5.0.0-pre.18",
		"@snickbit/feathers-helpers": "^0.0.43",
		"@snickbit/utilities": "^3.0.17",
		"ioredis": "^5.0.4",
		"sift": "^16.0.0"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.18",
		"@remedyred/indexer": "^2.1.1",
		"jest": "^27.5.1",
		"tsup": "^5.12.7"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	},
	"nx": {
		"implicitDependencies": [
			"@snickbit/feathers-helpers",
			"@snickbit/utilities"
		]
	}
}
