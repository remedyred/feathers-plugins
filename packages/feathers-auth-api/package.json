{
	"name": "@snickbit/feathers-auth-api",
	"description": "FeathersJS API Key Authentication",
	"version": "0.0.24",
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"engines": {
		"node": ">= 12"
	},
	"scripts": {
		"clean": "shx rm -rf ./dist",
		"build": "tsup",
		"meta": "pnpm run prod --metafile",
		"prod": "pnpm run build --minify",
		"dev": "pnpm run build --sourcemap",
		"watch": "pnpm run dev --watch",
		"depcheck": "depcheck"
	},
	"dependencies": {
		"@feathersjs/authentication": "5.0.0-pre.20",
		"@feathersjs/errors": "5.0.0-pre.20"
	},
	"devDependencies": {
		"@feathersjs/feathers": "5.0.0-pre.20",
		"@types/jest": "^27.4.1",
		"jest": "^27.5.1",
		"shx": "^0.3.4",
		"ts-jest": "^27.1.4",
		"tsup": "^5.12.7",
		"typescript": "^4.6.3"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
